{
  "request": {
    "method": "GET",
    "urlPattern": "/availability\\?time=.*"
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "time": "{{request.query.time}}",
      "available": "{{randomValue type='BOOLEAN'}}"
    }
  }
}
